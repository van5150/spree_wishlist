<%= form_for @wishlist, html: { class: 'ajax_form', id: 'change_wishlist_accessibility' } do |f| %>
  <%= f.radio_button :is_private, true %>
  <%= f.label 'is_private_true' do %><%= Spree.t(:private)  %><% end %>
  <%= f.radio_button :is_private, false %>
  <%= f.label 'is_private_false' do %><%= Spree.t(:public) %><% end %>
<% end -%>
